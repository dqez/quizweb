@model quizweb.Models.ApplicationUser
@{
    ViewData["Title"] = "Profile";
}
<div>
    <h1>Profile</h1>
    <div class="container">
        <div>
            <h2>@Model.UserName</h2>
            <p><strong>Full Name:</strong> @Model.FullName</p>
            <p><strong>Email:</strong> @Model.Email</p>
            <p><strong>Birthday:</strong> @Model.BirthDay</p>
        </div>
    </div>
</div>